<launch>
    <!-- Arguments -->
    <arg name="slam_methods" default="bag_cartographer" doc="slam type [gmapping, online_cartographer, offline_cartographer, bag_cartographer]"/>

    <!-- SLAM: Gmapping, Cartographer, Hector -->
    <include file="$(find agv_slam)/launch/$(arg slam_methods).launch"/>

    <!-- rviz -->
    <node name="rviz" pkg="rviz" type="rviz" required="true" args="-d $(find agv_slam)/rviz_config/agv_online_cartographer.rviz"/>
</launch>
